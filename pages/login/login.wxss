page {
  height: 100%;
  background-color: #3400CE;
}

.container {
  width: 100%;
  height: 100%;
}

.form-box {
  width: 670rpx;
  height: 907rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  margin-top: 247rpx;
  background-repeat: no-repeat;
  background-size: cover;
  background-image: url('data:image/png;base64,');
}

.title-container {
  width: 100%;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  /* margin-bottom: 94rpx;
  margin-top:21rpx; */
}

.title-text {
  font-size: 48rpx;
  color: #066CDE;
  width:100%;
  display:flex;
  justify-content: center;
  align-items: center;
  margin-top: 105rpx;
}

.row-wrap {
  width: 90%;
  display: flex;
  flex-direction: row;
  margin-top: 80rpx;
  margin-left:28rpx;
  margin-right:28rpx;
}

.input-phone {
  font-size: 28rpx;
  color: #333;
  padding-bottom: 2rpx;
  margin-top: 19rpx;
  margin-left: 18rpx;
  border-bottom: 2rpx solid #ebebeb;
  width:92%;
}

.input-code{
  flex:1;
  font-size: 28rpx;
  color: #333;
  padding-bottom: 2rpx;
  margin-top: 20rpx;
  margin-left: 10rpx;
  margin-right:10rpx;
  border-bottom: 2rpx solid #ebebeb;
}

.send {
  width: 180rpx;
  height: 52rpx;
  font-size: 26rpx;
  color: #8E8E8E;
  display: flex;
  align-items: center;
  justify-content: center;
  margin-top:20rpx;
  margin-right:4rpx;
  padding: 0;
}

.holder-input {
  font-size: 28rpx;
  color: #b2b2b2;
}

.codeBtn {
  font-size: 26rpx;
  width: 161rpx;
  height: 47rpx;
  border: 0.4rpx solid rgba(192, 192, 192, 1);
  border-radius: 6rpx;
}

.button-wrap {
  height: 80rpx;
  width: 90%;
  margin-top: 150rpx;
  background-repeat: no-repeat;
  background-color:#3A70F8;
  border-radius: 6rpx;
}

.button-wrap::after{
  border:2rpx solid #3A70F8;
}

.text-btn {
  font-size: 30rpx;
  text-align: center;
  line-height: 80rpx;
  color: white;
}
