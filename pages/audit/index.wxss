  justify-content: center;
  align-items:center;
  margin-left:80rpx;
  margin-right:80rpx;
}

.approveBtn {
  height:64rpx;
  background-color: #3400CE;
  font-size:30rpx;
  color: #FFFFFF;
  display: flex;
  justify-content: center;
  align-items:center;
  margin-left:80rpx;
  margin-right:80rpx;
}

.reason-wrapper {
  width:90%;
  height:70rpx;
  background-color:#E8E8E8;
