page {
  height: 100%;
  background-color: #3400ce;
}

.container {
  width: 100%;
  height: 100%;
}

.qrcode-box {
  width: 698rpx;
  height: 770rpx;
  display:flex;
  flex-direction: column;
  align-items: center;
  margin-bottom:298rpx;
  margin-left:26rpx;
  margin-right:26rpx;
  background-repeat: no-repeat;
  background-size:100% 100%;
  -moz-background-size:100% 100%;
  background-image: url('data:image/png;base64,')
}

.title {
  display:flex;
  height:104rpx;
  /* flex-direction: column; */
  justify-content: center;
  align-items: center;
  margin-top:15rpx;
  margin-left:244rpx;
  margin-right:244rpx;
  margin-bottom:10rpx;
}

.detail {
  width:100%;
  display:flex;
  height:452rpx;
  align-items: center;
  justify-content: center;
  flex-direction: column;
  margin-bottom:60rpx;
  /* background-color: red; */
}
.bottom-wrapper{
   width:100%;
  display:flex;
  flex-direction: column;
   align-items: center;

}

.bottom-btn {
  width:100%;
  display:flex;
  height:104rpx;
  justify-content: space-between;
}

.disapproveBtn {
  height:64rpx;
  background-color: #3400CE;
  font-size:30rpx;
  color: #FFFFFF;
